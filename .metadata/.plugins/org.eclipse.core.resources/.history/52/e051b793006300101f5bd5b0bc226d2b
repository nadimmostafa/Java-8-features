package org.nadim.collectionstream;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class MyStreamForEach {

	public static void main(String[] args) {
		
		//Iterate the stream element using forEach in lambda expression
		List<Integer> list = new ArrayList<>(); 
		list.add(15);
		list.add(9);
		list.add(5);
		list.add(22);
		list.add(17);
		
		list.stream().forEach(item->System.out.println(item));
		//list.stream().forEach(System.out::println);
		
		Map<String, Integer> fruits = new HashMap<>();	
		fruits.put("Apple",450);
		fruits.put("Mango",90);
		fruits.put("Banana",120);
		
		fruits.forEach((k,v)->System.out.print("Fruits name "+k+" price per KG "+v));
	
	}

}
