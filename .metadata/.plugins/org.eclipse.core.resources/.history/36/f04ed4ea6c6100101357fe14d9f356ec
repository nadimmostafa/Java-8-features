package org.nadim.collectionstream;

import java.util.ArrayList;
import java.util.List;

public class MyStreamSkip {

	public static void main(String[] args) {
		List<Employee> emplist = new ArrayList<>();
		emplist.add(new Employee(101,"Kafi","COO",80000,"Nator"));
		emplist.add(new Employee(105,"Nadim","CEO",250000,"Chapai"));
		emplist.add(new Employee(104,"Mostafa","Developer",50000,"Dhaka"));
		emplist.add(new Employee(102,"Atik","Manager",70000,"Dhaka"));
		emplist.add(new Employee(109,"Shuvo","Sr.Developer",65000,"Chapai"));
		emplist.add(new Employee(103,"Asif","CTO",150000,"Rajshai"));
		
		//display all elements except first two 
		//emplist.stream().skip(2).forEach(System.out::println);
		
		//display employee information whose is taking second highest salary 
		emplist.stream().sorted((e1,e2)->(e1.getSalary()<e2.getSalary()?1:e1.getSalary()==e2.getSalary()?0:-1)).limit(2).skip(1).forEach(System.out::println);
		
		
		List<String> names = new ArrayList<>();
		names.add("Nadim");
		names.add("Asif");
		names.add("Mostafa");
		names.add("Nadim");
		names.add("Shuvo");
		names.add("Asif");
		names.add("Atik");
		
		//names.stream().skip(3).forEach(System.out::println);
	}

}
