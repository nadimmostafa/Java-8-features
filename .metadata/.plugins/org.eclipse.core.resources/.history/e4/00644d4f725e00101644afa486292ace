package org.nadim.collectionenhancement;

public class Employee {

	String name;
	String designation;
	float salary;
	String city;
	
	public Employee(String name, String designation, float salary, String city) {
		super();
		this.name = name;
		this.designation = designation;
		this.salary = salary;
		this.city = city;
	}
	
}
